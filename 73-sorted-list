public boolean isSorted(List<Integer> numbers) {
    //code here
    if(numbers.size()<3){
        return true;
    }
    Boolean isAsc = numbers[0] < numbers[1];
    Integer lastNumber = numbers[0];
    for(Integer i=1;i<numbers.size();i++){
        if(numbers[i] == lastNumber){
            continue;
        }
        if(isAsc){
            if(numbers[i]>lastNumber){
                return false;
            }
        }else{
            if(numbers[i]<lastNumber){
                return false;
            }
        }

        lastNumber = numbers[i];
    }
    return true;
}