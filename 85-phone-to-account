public Map<String, Account> phoneToAccount(List<Account> accounts) {
    //code here
    Map<String, Account> phone2Account = new Map<String, Account>();
    for(Account acc : accounts){
        if(acc.Phone != null){
            phone2Account.put(acc.Phone, acc);
        }
    }
    System.debug(phone2Account.get('3210987654').AccountNumber);
    return phone2Account;
}